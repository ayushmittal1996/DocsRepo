	public static void pattern8() {

		for (int i = 1; i <= 7; i++) {
			for (int j = 1; j <= i; j++) {
				if ((i+j)>7) {
					break;
				}
				System.out.print("*");
			}
	
			System.out.println();
		}
	}
	
