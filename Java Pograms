	public static void pattern5() {
		int s = 0;
		for (int i = 1; i <= 4; i++) {
			for (int j = 1; j <= 4; j++) {

				if (j <= s) {
					System.out.print(" ");
				} else {
					System.out.print("*");
				}

			}
			s++;
			System.out.println();
		}
	}
