	public static void pattern7() {
		int s = 0;
		for (int i = 1; i <= 4; i++) {
			for (int j = 1; j <= i; j++) {
				if ((j + s) % 2 == 0)
					System.out.print(0);
				else
					System.out.print(1);
			}
			s++;
			System.out.println();
		}
	}
