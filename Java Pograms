		public static void pattern6() {
		int s = 0;
		for (int i = 1; i <= 4; i++) {
			for (int j = 1; j <= 4; j++) {
				if ((j + s) > 4)
					System.out.print(j + s - 4);
				else
					System.out.print(j + s);
			}
			s++;
			System.out.println();
		}
	}
