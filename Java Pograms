//Asci is of 7 bits
		//So range 2^7=128
		//Range from 0 to 127
		for (int i = 0; i <= 127; i++) {
			//%d numeric Values
			//%c character Values
			//printf For printing a format
			System.out.printf("%d : %c \n",i,i);
      ------------------------------------------
      public static void sort(int a[]) {
		for (int i = 0; i < a.length-1; i++) {
			for (int j = 0; j < a.length-1-i; j++) {
				if(a[j]<a[j+1]) {
					int b=a[j];
					a[j]=a[j+1];
					a[j+1]=b;
				}
			}
		}
    
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
	}
